	$(CXX_add2_ti_simulate) $(CXXFLAGS_add2_ti_simulate) --preproc_dependency=$*.libfoo_static_tisim_eabi_c67plus_c6run_add2_ti_simulate.d $(DEFINES_add2_ti_simulate) $(INCLUDES_add2_ti_simulate) -o$@ $<

CLEAN_COMPILE_add1_debug = $(BASE)example-2/src/foo/foo.libfoo_static_tisim_eabi_c67plus_c6run_add1_debug.d $(BASE)example-2/src/foo/foo_asm.libfoo_static_tisim_eabi_c67plus_c6run_add1_debug.d\
                           $(BASE)example-2/src/foo/foo_cpp.libfoo_static_tisim_eabi_c67plus_c6run_add1_debug.d
CLEAN_COMPILE_add1_release = $(BASE)example-2/src/foo/foo.libfoo_static_tisim_eabi_c67plus_c6run_add1_release.d $(BASE)example-2/src/foo/foo_asm.libfoo_static_tisim_eabi_c67plus_c6run_add1_release.d\
                             $(BASE)example-2/src/foo/foo_cpp.libfoo_static_tisim_eabi_c67plus_c6run_add1_release.d
CLEAN_COMPILE_add1_ti_simulate = $(BASE)example-2/src/foo/foo.libfoo_static_tisim_eabi_c67plus_c6run_add1_ti_simulate.d $(BASE)example-2/src/foo/foo_asm.libfoo_static_tisim_eabi_c67plus_c6run_add1_ti_simulate.d\
                                 $(BASE)example-2/src/foo/foo_cpp.libfoo_static_tisim_eabi_c67plus_c6run_add1_ti_simulate.d
CLEAN_COMPILE_add2_debug = $(BASE)example-2/src/foo/foo.libfoo_static_tisim_eabi_c67plus_c6run_add2_debug.d $(BASE)example-2/src/foo/foo_asm.libfoo_static_tisim_eabi_c67plus_c6run_add2_debug.d\
                           $(BASE)example-2/src/foo/foo_cpp.libfoo_static_tisim_eabi_c67plus_c6run_add2_debug.d
CLEAN_COMPILE_add2_release = $(BASE)example-2/src/foo/foo.libfoo_static_tisim_eabi_c67plus_c6run_add2_release.d $(BASE)example-2/src/foo/foo_asm.libfoo_static_tisim_eabi_c67plus_c6run_add2_release.d\
                             $(BASE)example-2/src/foo/foo_cpp.libfoo_static_tisim_eabi_c67plus_c6run_add2_release.d
CLEAN_COMPILE_add2_ti_simulate = $(BASE)example-2/src/foo/foo.libfoo_static_tisim_eabi_c67plus_c6run_add2_ti_simulate.d $(BASE)example-2/src/foo/foo_asm.libfoo_static_tisim_eabi_c67plus_c6run_add2_ti_simulate.d\
                                 $(BASE)example-2/src/foo/foo_cpp.libfoo_static_tisim_eabi_c67plus_c6run_add2_ti_simulate.d

cleandeps:
	$(RM) $(CLEAN_COMPILE_add1_debug)
	$(RM) $(CLEAN_COMPILE_add1_release)
	$(RM) $(CLEAN_COMPILE_add1_ti_simulate)
	$(RM) $(CLEAN_COMPILE_add2_debug)
	$(RM) $(CLEAN_COMPILE_add2_release)
	$(RM) $(CLEAN_COMPILE_add2_ti_simulate)

clean: cleanself

cleanself:
	$(RM) $(CLEAN_COMPILE_add1_debug)
	$(RM) $(CLEAN_COMPILE_add1_release)
	$(RM) $(CLEAN_COMPILE_add1_ti_simulate)
	$(RM) $(CLEAN_COMPILE_add2_debug)
	$(RM) $(CLEAN_COMPILE_add2_release)
	$(RM) $(CLEAN_COMPILE_add2_ti_simulate)
	$(RM) $(INPUTS_ARCHIVE_add1_debug)
	$(RM) $(INPUTS_ARCHIVE_add2_debug)
	$(RM) $(INPUTS_ARCHIVE_add1_release)
	$(RM) $(INPUTS_ARCHIVE_add2_release)
	$(RM) $(INPUTS_ARCHIVE_add1_ti_simulate)
	$(RM) $(INPUTS_ARCHIVE_add2_ti_simulate)
	$(RM) libfoo_static_add1_debug.lib
	$(RM) libfoo_static_add2_debug.lib
	$(RM) libfoo_static_add1_release.lib
	$(RM) libfoo_static_add2_release.lib
	$(RM) libfoo_static_add1_ti_simulate.lib
	$(RM) libfoo_static_add2_ti_simulate.lib

.PHONY: default FORCE clean cleandeps cleanself help

FORCE:

help:
	@echo "This makefile has the following targets:"
	@echo " libfoo_static_add1_debug.lib"
	@echo " libfoo_static_add2_debug.lib"
	@echo " libfoo_static_add1_release.lib"
	@echo " libfoo_static_add2_release.lib"
	@echo " libfoo_static_add1_ti_simulate.lib"
	@echo " libfoo_static_add2_ti_simulate.lib"
	@echo " default"
	@echo " clean"
	@echo " cleandeps"
	@echo " help"

