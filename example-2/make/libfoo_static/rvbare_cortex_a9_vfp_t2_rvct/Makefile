	$(CCDEP_add1_arm_simulate) --cpu=Cortex-A9.no_neon --mm $(DEFINES_add1_arm_simulate) $(INCLUDES_add1_arm_simulate) -o $@ > $*.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add1_arm_simulate.d $<
	$(CC_add1_arm_simulate) $(CFLAGS_add1_arm_simulate) $(DEFINES_add1_arm_simulate) $(INCLUDES_add1_arm_simulate) -o $@ $<

