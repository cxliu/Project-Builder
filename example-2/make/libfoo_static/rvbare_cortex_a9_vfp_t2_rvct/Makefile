#-*-makefile-*-

BASE=../../../../
ARFLAGS_add1_arm_simulate = -rc
ARFLAGS_add1_debug = -rc
ARFLAGS_add1_release = -rc
ARFLAGS_add2_arm_simulate = -rc
ARFLAGS_add2_debug = -rc
ARFLAGS_add2_release = -rc
AR_add1_arm_simulate = armar
AR_add1_debug = armar
AR_add1_release = armar
AR_add2_arm_simulate = armar
AR_add2_debug = armar
AR_add2_release = armar
CCDEP_add1_arm_simulate = armcc
CCDEP_add1_debug = armcc
CCDEP_add1_release = armcc
CCDEP_add2_arm_simulate = armcc
CCDEP_add2_debug = armcc
CCDEP_add2_release = armcc
CC_add1_arm_simulate = armcc
CC_add1_debug = armcc
CC_add1_release = armcc
CC_add2_arm_simulate = armcc
CC_add2_debug = armcc
CC_add2_release = armcc
CFLAGS_add1_arm_simulate = --cpu=Cortex-A9.no_neon --fpu=VFPv3_D16 --thumb -g -O3 -Otime --apcs=/interwork -c --c99
CFLAGS_add1_debug = --cpu=Cortex-A9.no_neon --fpu=VFPv3_D16 --thumb -g -O0 --apcs=/interwork -c --c99
CFLAGS_add1_release = --cpu=Cortex-A9.no_neon --fpu=VFPv3_D16 --thumb -O3 -Otime --apcs=/interwork -c --c99
CFLAGS_add2_arm_simulate = --cpu=Cortex-A9.no_neon --fpu=VFPv3_D16 --thumb -g -O3 -Otime --apcs=/interwork -c --c99
CFLAGS_add2_debug = --cpu=Cortex-A9.no_neon --fpu=VFPv3_D16 --thumb -g -O0 --apcs=/interwork -c --c99
CFLAGS_add2_release = --cpu=Cortex-A9.no_neon --fpu=VFPv3_D16 --thumb -O3 -Otime --apcs=/interwork -c --c99
CXXFLAGS_add1_arm_simulate = --cpu=Cortex-A9.no_neon --fpu=VFPv3_D16 --thumb -g -O3 -Otime --apcs=/interwork -c
CXXFLAGS_add1_debug = --cpu=Cortex-A9.no_neon --fpu=VFPv3_D16 --thumb -g -O0 --apcs=/interwork -c
CXXFLAGS_add1_release = --cpu=Cortex-A9.no_neon --fpu=VFPv3_D16 --thumb -O3 -Otime --apcs=/interwork -c
CXXFLAGS_add2_arm_simulate = --cpu=Cortex-A9.no_neon --fpu=VFPv3_D16 --thumb -g -O3 -Otime --apcs=/interwork -c
CXXFLAGS_add2_debug = --cpu=Cortex-A9.no_neon --fpu=VFPv3_D16 --thumb -g -O0 --apcs=/interwork -c
CXXFLAGS_add2_release = --cpu=Cortex-A9.no_neon --fpu=VFPv3_D16 --thumb -O3 -Otime --apcs=/interwork -c
CXX_add1_arm_simulate = armcc
CXX_add1_debug = armcc
CXX_add1_release = armcc
CXX_add2_arm_simulate = armcc
CXX_add2_debug = armcc
CXX_add2_release = armcc
DEFINES_add1_arm_simulate = -DADD_THIS=1 -DNDEBUG=1
DEFINES_add1_debug = -DADD_THIS=1
DEFINES_add1_release = -DADD_THIS=1 -DNDEBUG=1
DEFINES_add2_arm_simulate = -DADD_THIS=2 -DNDEBUG=1
DEFINES_add2_debug = -DADD_THIS=2
DEFINES_add2_release = -DADD_THIS=2 -DNDEBUG=1
INCLUDES_add1_arm_simulate = -I$(BASE).
INCLUDES_add1_debug = -I$(BASE).
INCLUDES_add1_release = -I$(BASE).
INCLUDES_add2_arm_simulate = -I$(BASE).
INCLUDES_add2_debug = -I$(BASE).
INCLUDES_add2_release = -I$(BASE).


INPUTS_ARCHIVE_add1_arm_simulate = $(BASE)example-2/src/foo/foo.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add1_arm_simulate.o\
                                   $(BASE)example-2/src/foo/foo_asm.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add1_arm_simulate.o\
                                   $(BASE)example-2/src/foo/foo_cpp.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add1_arm_simulate.o
INPUTS_ARCHIVE_add1_debug = $(BASE)example-2/src/foo/foo.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add1_debug.o $(BASE)example-2/src/foo/foo_asm.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add1_debug.o\
                            $(BASE)example-2/src/foo/foo_cpp.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add1_debug.o
INPUTS_ARCHIVE_add1_release = $(BASE)example-2/src/foo/foo.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add1_release.o $(BASE)example-2/src/foo/foo_asm.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add1_release.o\
                              $(BASE)example-2/src/foo/foo_cpp.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add1_release.o
INPUTS_ARCHIVE_add2_arm_simulate = $(BASE)example-2/src/foo/foo.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add2_arm_simulate.o\
                                   $(BASE)example-2/src/foo/foo_asm.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add2_arm_simulate.o\
                                   $(BASE)example-2/src/foo/foo_cpp.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add2_arm_simulate.o
INPUTS_ARCHIVE_add2_debug = $(BASE)example-2/src/foo/foo.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add2_debug.o $(BASE)example-2/src/foo/foo_asm.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add2_debug.o\
                            $(BASE)example-2/src/foo/foo_cpp.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add2_debug.o
INPUTS_ARCHIVE_add2_release = $(BASE)example-2/src/foo/foo.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add2_release.o $(BASE)example-2/src/foo/foo_asm.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add2_release.o\
                              $(BASE)example-2/src/foo/foo_cpp.libfoo_static_rvbare_cortex_a9_vfp_t2_rvct_add2_release.o


default: libfoo_static_add1_arm_simulate.lib libfoo_static_add2_arm_simulate.lib libfoo_static_add1_debug.lib libfoo_static_add2_debug.lib libfoo_static_add1_release.lib libfoo_static_add2_release.lib

libfoo_static_add1_arm_simulate.lib: $(INPUTS_ARCHIVE_add1_arm_simulate)
	$(AR_add1_arm_simulate) $(ARFLAGS_add1_arm_simulate)   $@ $^

libfoo_static_add2_arm_simulate.lib: $(INPUTS_ARCHIVE_add2_arm_simulate)
	$(AR_add2_arm_simulate) $(ARFLAGS_add2_arm_simulate)   $@ $^

libfoo_static_add1_debug.lib: $(INPUTS_ARCHIVE_add1_debug)
	$(AR_add1_debug) $(ARFLAGS_add1_debug)   $@ $^

libfoo_static_add2_debug.lib: $(INPUTS_ARCHIVE_add2_debug)
	$(AR_add2_debug) $(ARFLAGS_add2_debug)   $@ $^

libfoo_static_add1_release.lib: $(INPUTS_ARCHIVE_add1_release)
	$(AR_add1_release) $(ARFLAGS_add1_release)   $@ $^

libfoo_static_add2_release.lib: $(INPUTS_ARCHIVE_add2_release)
	$(AR_add2_release) $(ARFLAGS_add2_release)   $@ $^

